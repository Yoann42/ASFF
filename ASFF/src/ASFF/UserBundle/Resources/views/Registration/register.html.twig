{# src/ASFF/UserBundle/Resources/views/Registration/register.html.twig #}

{% extends "ASFFUserBundle::layout.html.twig" %}

{% block fos_user_content %}
  
<br>
  
{# Le formulaire d'inscription #}
<form class="panel panel-primary" action="{{ path('fos_user_registration_register') }}" method="POST">
	<div class="panel-heading">
		<h2>Inscription</h2>
	</div>
	<div class="panel-body">
		{{ form_widget(form) }}
 		<div><input type="submit" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}" /></div>
	</div>
</form>
  
  

{% endblock fos_user_content %}